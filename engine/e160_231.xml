    <svcsubsec>
        <title>ALTERNATOR BRACKET</title>
        <svcproc svcproctype="install" graphiccount="3">
            <title>INSTALLATION</title>
            <step1>
                <para count="1">Install alternator support bracket to front cover and secure with capscrews coated with
                    Loctite(r) 262.
                </para>
                <figure id="ef1600835" figsize="col-wide">
                    <graphic id="eg6171" filename="../figeg6/EG6171.webp" type="webp"/>
