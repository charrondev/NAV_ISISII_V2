    <svcman/>
    <svcsubsec id="s210011104">
        <title>Inspect and Test</title>
        <svcproc id="s210011104.1" svcproctype="inspect" graphiccount="1">
            <title>Inspect ECM</title>
            <step1>
                <para count="1">Inspect ECM mounting bushings and pins for cracks, cuts, and worn areas. Replace as
                    required.
                </para>
            </step1>
            <step1>
                <para count="1">Inspect engine wiring harness connectors, connector covers and cover shields for cracks,
                    cuts, or worn areas. Replace as required.
                </para>
            </step1>
            <figure id="f210011130" figsize="col-wide">
                <graphic filename="../figeg8/EG8247.webp" type="webp" scalefit="1" scalefitwidth="248px"/>
                <caption>ECM bushing and pin</caption>
                <calloutgroup>
                    <callout1 type="arabic">Washer</callout1>
                    <callout1 type="arabic">Bushing</callout1>
                    <callout1 type="arabic">Pin</callout1>
                </calloutgroup>
            </figure>
        </svcproc>
    </svcsubsec>
