    </stitle>
    <svcman/>
    <svcproc svcproctype="remove" graphiccount="2">
        <title>Oil Pickup Tube</title>
        <figure id="f2350603" figsize="col-wide">
            <graphic filename="../figd06/D0611.webp" type="webp" scalefit="1" scalefitwidth="248px" box="no"/>
            <caption>Figure 250. Oil pickup tube</caption>
            <calloutgroup>
                <callout1 type="arabic">Oil pickup tube mounting bolt M6 X 16 (3)</callout1>
            </calloutgroup>
        </figure>
        <step1>
            <para count="1">Remove three mounting bolts from oil pickup tube.</para>
        </step1>
        <figure id="f2350603.5" figsize="col-wide">
            <graphic filename="../figd06/D0627.webp" type="webp" scalefit="1" scalefitwidth="248px" box="no"/>
            <caption>Figure 251. O-ring</caption>
            <calloutgroup>
                <callout1 type="arabic">Oil pickup tube O-ring</callout1>
            </calloutgroup>
        </figure>
        <step1>
            <para count="1">Remove O-ring from oil pickup tube and discard.</para>
        </step1>
    </svcproc>
