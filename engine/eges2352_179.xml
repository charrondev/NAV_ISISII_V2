<isis lang="en">
    <stitle>INTERNATIONAL&#174; VT 365 DIESEL ENGINE - ENGINE ELECTRICAL - Removal - Exhaust Back Pressure (EBP)
        Sensor
    </stitle>
    <svcman/>
    <svcproc id="s23511.3.3" svcproctype="remove" graphiccount="1">
        <title>Exhaust Back Pressure (EBP) Sensor</title>
        <figure id="f2351018" figsize="col-wide">
            <graphic filename="../figd09/D0909.webp" type="webp" scalefit="1" scalefitwidth="248px" box="no"/>
            <caption>Figure 373. EBP sensor</caption>
            <calloutgroup>
                <callout1 type="arabic">EBP sensor</callout1>
                <callout1 type="arabic">EBP sensor connector</callout1>
            </calloutgroup>
        </figure>
        <step1>
            <para count="1">Disconnect 3-pin electrical connector from EBP sensor mounted on a bracket on the left side
                of ECM / IDM bracket.
            </para>
        </step1>
        <step1>
            <para count="1">Unthread fitting on EBP tubing and remove EBP sensor.</para>
        </step1>
    </svcproc>
