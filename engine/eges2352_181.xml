    </stitle>
    <svcman/>
    <svcproc id="s23511.3.5" svcproctype="remove" graphiccount="1">
        <title>Engine Oil Temperature (EOT) and Engine Oil Pressure (EOP) Sensors</title>
        <figure id="f2351021" figsize="col-wide">
            <graphic filename="../figd09/D0920.webp" type="webp" scalefit="1" scalefitwidth="248px" box="no"/>
