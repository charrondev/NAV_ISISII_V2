                            <entry>
                                <para>Ambient temperature</para>
                            </entry>
                            <entry>
                                <para>Intake Air Temp</para>
                            </entry>
                        </row>
                        <row>
                            <entry>
                                <para>Coolant temperature.</para>
                            </entry>
                            <entry>
                                <para>Coolant Temp</para>
                            </entry>
                        </row>
                        <row>
                            <entry>
                                <para>Engine SN</para>
                            </entry>
                            <entry>
                                <para>Engine Serial Number</para>
                            </entry>
                        </row>
                        <row>
                            <entry>
                                <para>Engine HP</para>
                            </entry>
