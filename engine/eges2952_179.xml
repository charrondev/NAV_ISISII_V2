<isis lang="en">
    <stitle>VT 365 Diesel Engine - 2004 Model Year - OIL COOLER and FILTER HOUSING - Removal - Oil Cooler Cover
        Assembly
    </stitle>
    <svcman/>
    <svcproc id="s26509.2.3" svcproctype="remove" graphiccount="4">
        <title>Oil Cooler Cover Assembly</title>
        <figure id="f2650910" figsize="col-wide">
            <graphic filename="../figd13/D13114.webp" type="webp" scalefit="1" scalefitwidth="248px"
                     scalefitheight="312px" box="no"/>
            <caption>Mounting bolts for oil cooler cover assembly</caption>
        </figure>
        <step1>
            <para count="1">Remove ten M8 x 30 oil cooler cover assembly mounting bolts.</para>
        </step1>
        <figure id="f2650912" figsize="col-wide">
            <graphic filename="../figd13/D13116.webp" type="webp" scalefit="1" scalefitwidth="248px"
                     scalefitheight="312px" box="no"/>
            <caption>Cooler cover assembly</caption>
        </figure>
        <figure id="f2650913" figsize="col-wide">
            <graphic filename="../figd13/D13117.webp" type="webp" scalefit="1" scalefitwidth="248px"
                     scalefitheight="312px" box="no"/>
            <caption>Reservoir cover gasket</caption>
        </figure>
        <step1>
            <para count="1">Remove oil cooler cover assembly from crankcase.</para>
        </step1>
        <step1>
            <para count="1">Remove and discard reservoir cover gasket.</para>
        </step1>
        <step1>
            <para count="1">Remove oil from reservoir.</para>
        </step1>
        <figure id="f2650914" figsize="col-wide">
            <graphic filename="../figd13/D1381.webp" type="webp" scalefit="1" scalefitwidth="248px"
                     scalefitheight="312px" box="no"/>
            <caption>Pump inlet strainer</caption>
        </figure>
        <step1>
            <para count="1">Remove pump inlet strainer from oil reservoir.</para>
        </step1>
    </svcproc>
