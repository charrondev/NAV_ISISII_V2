<isis lang="en">
    <stitle>VT 365 Diesel Engine - 2004 Model Year - ENGINE ELECTRICAL - Removal - Exhaust Back Pressure (EBP) Sensor
    </stitle>
    <svcman/>
    <svcproc id="s26510.3.3" svcproctype="remove" graphiccount="1">
        <title>Exhaust Back Pressure (EBP) Sensor</title>
        <figure id="f2651019" figsize="col-wide">
            <graphic filename="../figd09/D0978.webp" type="webp" scalefit="1" scalefitwidth="248px"
                     scalefitheight="312px" box="no"/>
            <caption>EBP sensor</caption>
            <calloutgroup>
                <callout1 type="arabic">EBP sensor</callout1>
                <callout1 type="arabic">EBP sensor connector</callout1>
            </calloutgroup>
        </figure>
        <step1>
            <para count="1">Disconnect the 3-pin electrical connector from the EBP sensor mounted on the left side of
                the engine below the ECM.
            </para>
        </step1>
        <step1>
            <para count="1">Remove EBP sensor.</para>
        </step1>
    </svcproc>
