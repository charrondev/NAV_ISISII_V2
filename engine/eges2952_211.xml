<isis lang="en">
    <stitle>VT 365 Diesel Engine - 2004 Model Year - ENGINE ELECTRICAL - Removal - Engine Coolant Temperature (ECT)
        Sensor
    </stitle>
    <svcman/>
    <svcproc id="s26510.3.4" svcproctype="remove" graphiccount="1">
        <title>Engine Coolant Temperature (ECT) Sensor</title>
        <figure id="f2651020" figsize="col-wide">
            <graphic filename="../figd09/D0910.webp" type="webp" scalefit="1" scalefitwidth="248px"
                     scalefitheight="312px" box="no"/>
            <caption>ECT sensor</caption>
            <calloutgroup>
                <callout1 type="arabic">ECT sensor</callout1>
                <callout1 type="arabic">ECT sensor connector</callout1>
            </calloutgroup>
        </figure>
        <step1>
            <para count="1">Disconnect the 2-pin electrical connector from the ECT sensor installed in the left side of
                the front cover.
            </para>
        </step1>
        <step1>
            <para count="1">Remove ECT sensor and discard O-ring.</para>
        </step1>
    </svcproc>
