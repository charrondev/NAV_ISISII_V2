<isis lang="en">
    <stitle>VT 365 Diesel Engine - 2004 Model Year - ENGINE ELECTRICAL - Removal - Manifold Absolute Pressure (MAP)
        Sensor
    </stitle>
    <svcman/>
    <svcproc id="s26510.3.10" svcproctype="remove" graphiccount="1">
        <title>Manifold Absolute Pressure (MAP) Sensor</title>
        <figure id="f2651035" figsize="col-wide">
            <graphic filename="../figd09/D0908.webp" type="webp" scalefit="1" scalefitwidth="248px"
                     scalefitheight="312px" box="no"/>
            <caption>MAP sensor</caption>
            <calloutgroup>
                <callout1 type="arabic">MAP sensor connector</callout1>
                <callout1 type="arabic">MAP sensor</callout1>
            </calloutgroup>
        </figure>
        <step1>
            <para count="1">Disconnect 3-pin electrical connector from the MAP sensor installed on top of the intake
                manifold.
            </para>
        </step1>
        <step1>
            <para count="1">Remove MAP sensor and discard O-ring.</para>
        </step1>
    </svcproc>
