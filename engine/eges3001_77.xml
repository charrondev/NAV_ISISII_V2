    <svcproc id="s26504.1.2" graphiccount="4">
        <title>Service Procedure</title>
        <svcsubproc id="s26504.1.2.1">
            <title>Cylinder Head Component Locations and Exploded Views</title>
            <figure id="e300501" figsize="pg-wide">
                <graphic filename="../figj08/J08093.webp" type="webp" scalefit="1" scalefitwidth="484px" box="no"/>
                <caption>Exploded view of cylinder head related components</caption>
                <calloutgroup>
                    <callout1 type="arabic">O-ring seal</callout1>
                    <callout1 type="arabic">Crankcase vent hose assembly (CF series)</callout1>
                    <callout1 type="arabic">Crankcase vent hose assembly (stripped chassis)</callout1>
                    <callout1 type="arabic">Bolt, M10 x 16, (to back of cyl head) (stripped chassis)</callout1>
                    <callout1 type="arabic">Bolt, M6 x 43 (6)</callout1>
                    <callout1 type="arabic">Valve cover (left side)</callout1>
                    <callout1 type="arabic">Rocker arm carrier (2)</callout1>
                    <callout1 type="arabic">Bolt, M8 x 70 (8)</callout1>
                    <callout1 type="arabic">Cylinder head assembly (2)</callout1>
                    <callout1 type="arabic">Rocker arm carrier gasket (2)</callout1>
                    <callout1 type="arabic">Valve cover gasket (2)</callout1>
                    <callout1 type="arabic">Valve cover bolt (18)</callout1>
                    <callout1 type="arabic">Breather housing assembly</callout1>
                    <callout1 type="arabic">ICP sensor seal</callout1>
                    <callout1 type="arabic">Oil fill cap</callout1>
                    <callout1 type="arabic">Valve cover (right side)</callout1>
                </calloutgroup>
            </figure>
            <figure id="e300502" figsize="pg-wide">
                <graphic filename="../figj08/J08087.webp" type="webp" scalefit="1" scalefitwidth="502px" box="no"/>
                <caption>Cylinder head components</caption>
                <calloutgroup>
                    <callout1 type="arabic">Hydraulic cam follower (12)</callout1>
                    <callout1 type="arabic">Cam follower guide (2)</callout1>
                    <callout1 type="arabic">Bolt, M8 x 70 (8)</callout1>
                    <callout1 type="arabic">Push rod (12)</callout1>
                    <callout1 type="arabic">Valve bridges (12)</callout1>
                    <callout1 type="arabic">Valve retainer keys (48)</callout1>
                    <callout1 type="arabic">Valve spring retainer (24)</callout1>
                    <callout1 type="arabic">Valve spring (24)</callout1>
                    <callout1 type="arabic">Valve stem oil seal (24)</callout1>
                    <callout1 type="arabic">Fuel injector sleeve (6)</callout1>
                    <callout1 type="arabic">Glow plug sleeve (6)</callout1>
                    <callout1 type="arabic">Core plug (10)</callout1>
                    <callout1 type="arabic">Exhaust valves (12)</callout1>
                    <callout1 type="arabic">Intake valves (12)</callout1>
                    <callout1 type="arabic">Cylinder head (2)</callout1>
                    <callout1 type="arabic">Bolt/washer (2)</callout1>
                </calloutgroup>
            </figure>
            <figure id="e300503" figsize="pg-wide">
                <graphic filename="../figJ08/J08094.webp" type="webp" scalefit="1" scalefitwidth="514px" box="no"/>
                <caption>Rocker arm carrier components</caption>
                <calloutgroup>
                    <callout1 type="arabic">Head bolt, M14 x 193 (16)</callout1>
                    <callout1 type="arabic">Fulcrum bolt spacer (12)</callout1>
                    <callout1 type="arabic">Bolt, M8 X 45 (12)</callout1>
                    <callout1 type="arabic">Rocker arm clip (12)</callout1>
                    <callout1 type="arabic">Ball, &#x215C; chrome (12)</callout1>
                    <callout1 type="arabic">Rocker arm (intake) (6)</callout1>
                    <callout1 type="arabic">Dowel sleeve (end fulcrums only) (4)</callout1>
                    <callout1 type="arabic">Fulcrum plate (exhaust) (2)</callout1>
                    <callout1 type="arabic">Rocker arm (exhaust) (6)</callout1>
                    <callout1 type="arabic">Self tapping bolt stud, M10 x 30 x M8 x 10 (CF only)</callout1>
                    <callout1 type="arabic">Fulcrum plate (dual) (4)</callout1>
                    <callout1 type="arabic">Dowel sleeve bushing (4)</callout1>
                    <callout1 type="arabic">Bolt, M8 X 50 (4)</callout1>
                    <callout1 type="arabic">Fulcrum plate (intake) (2)</callout1>
                </calloutgroup>
            </figure>
            <figure id="e300504" figsize="pg-wide">
                <graphic filename="../figj08/J08032.webp" type="webp" scalefit="1" scalefitwidth="472px" box="no"/>
                <caption>Cylinder head components locations</caption>
                <calloutgroup>
                    <callout1 type="arabic">Rocker arm fulcrum plate (exhaust) (1 per head)</callout1>
                    <callout1 type="arabic">Exhaust rocker arm (long) (3 per head)</callout1>
                    <callout1 type="arabic">Injector assembly (3 per head)</callout1>
                    <callout1 type="arabic">Glow plug (3 per head)</callout1>
                    <callout1 type="arabic">Intake rocker arm (short) (3 per head)</callout1>
                    <callout1 type="arabic">Case-to-head tube assembly (1 per head)</callout1>
                    <callout1 type="arabic">Valve bridge (2 per cylinder)</callout1>
                    <callout1 type="arabic">Rocker arm fulcrum plate (dual) (2 per head)</callout1>
                    <callout1 type="arabic">Injector harness connector (3 per head)</callout1>
                    <callout1 type="arabic">Rocker arm fulcrum plate (intake) (1 per head)</callout1>
                </calloutgroup>
            </figure>
            <specpara>
                <note>
                    <para>Before removing cylinder heads, the following components must be removed first. See the
                        appropriate section for removal procedure:
                    </para>
                    <randlist type="bullet">
                        <item>
                            <para>Fuel filter assembly and lines</para>
                        </item>
                        <item>
                            <para>Turbocharger assembly and tubing</para>
                        </item>
                        <item>
                            <para>Intake manifold and EGR cooler</para>
                        </item>
                        <item>
                            <para>Flywheel and rear cover (only if removing cam followers)</para>
                        </item>
                        <item>
                            <para>Tube assembly, rear engine branch (only if removing cam followers)</para>
                        </item>
                    </randlist>
                </note>
            </specpara>
        </svcsubproc>
    </svcproc>
