<isis lang="en">
    <stitle>International&#174; MaxxForce&#8482; DT, 9 and 10 Engine Service Manual - Diamond Logic&#174; Engine Brake -
        Removal - Brake Actuator Piston Assembly
    </stitle>
    <svcman/>
    <svcproc svcproctype="remove" graphiccount="3">
        <title>Brake Actuator Piston Assembly</title>
        <figure id="f3350008216" figsize="col-wide">
            <graphic filename="../figh08/H08095.webp" type="webp" scalefit="1" scalefitwidth="248px" box="no"/>
            <caption>Brake actuator piston locknut</caption>
        </figure>
        <step1>
            <para count="1">Remove brake actuator piston locknut.</para>
        </step1>
        <figure id="f3350008217" figsize="col-wide">
            <graphic filename="../figh08/H08096.webp" type="webp" scalefit="1" scalefitwidth="248px" box="no"/>
            <caption>Brake actuator piston removal</caption>
        </figure>
        <step1>
            <para count="1">Remove brake actuator piston assembly from the high-pressure oil manifold.</para>
        </step1>
        <figure id="f3350008218" figsize="col-wide">
            <graphic filename="../figh08/H08097.webp" type="webp" scalefit="1" scalefitwidth="248px" box="no"/>
            <caption>Brake actuator piston assembly</caption>
        </figure>
        <step1>
            <para count="1">Remove brake actuator piston.</para>
        </step1>
        <specpara>
            <note>
                <para>There are no seals associated with the brake actuator piston assembly.</para>
            </note>
        </specpara>
    </svcproc>
