        <figure id="f3350003080" figsize="col-wide">
            <graphic filename="../figk09/K09013.webp" type="webp" scalefit="1" scalefitwidth="248px" box="no"/>
            <caption>VGT actuator connector</caption>
            <calloutgroup>
