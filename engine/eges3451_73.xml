    <svcman/>
    <svcproc id="s03.5.2" svcproctype="install" graphiccount="1">
        <title>Fuel Rail Pressure (FRP) Sensor</title>
        <figure id="f3450003039" figsize="col-wide">
            <graphic filename="../figm09/m09106.webp" type="webp" scalefit="1" scalefitwidth="248px"
                     scalefitheight="192px" box="no"/>
            <caption>FRP sensor</caption>
        </figure>
        <step1>
            <para count="1">Install FRP sensor. Tighten sensor to
                <hotlnk document="eges3451_90.xml" xrefid="s03.t1">special torque</hotlnk>
                <xref refid="s03.t1"/>.
            </para>
        </step1>
        <step1>
            <para count="1">If Under Valve Cover (UVC) harness not previously removed, connect electrical connector to
                FRP sensor.
            </para>
        </step1>
    </svcproc>
