    <stitle>AIR COMPRESSOR: MIDLAND MODELS EL-1300 AND EL-1600</stitle>
    <svcsection/>
    <svcsection id="s04011" svcsectype="prodspec" division="truck" formnum="CTS-5146" date="03/31/1996">
        <title>AIR COMPRESSOR: MIDLAND MODELS EL-1300 AND EL-1600</title>
        <apply>
            <configure>
                <vendorgroup/>

                <vendor>MIDLAND</vendor>
                <ucodegroup/>

                <ucode>04SMB</ucode>
                <ucode>04SMC</ucode>
                <vendcodegroup/>

                <vendcode>EL-1300</vendcode>
                <vendcode>EL-1600</vendcode>
            </configure>
            <configure>
                <truckmodelgroup/>

                <truckmodel>2000</truckmodel>
                <truckmodel>3000</truckmodel>
                <truckmodel>4000</truckmodel>
                <truckmodel>5000</truckmodel>
                <truckmodel>7000</truckmodel>
                <truckmodel>9100</truckmodel>
                <truckmodel>9200</truckmodel>
                <truckmodel>9300</truckmodel>
