    <stitle>POWER STEERING GEAR: SHEPPARD M-SERIES - IDENTIFICATION</stitle>
    <svcsection/>
    <svcproc id="hl0500207" svcproctype="IDcomponent" graphiccount="4">
        <title>IDENTIFICATION</title>
        <para>Refer to
            <hotlnk xrefid="f0500201">Figure 1</hotlnk>.
        </para>
        <figure id="f0500201" figsize="pg-wide">
            <graphic id="g0500201" filename="../figg05/G0500201.webp" type="webp" scalefit="1" scalefitwidth="514px"
                     scalefitheight="592px"/>
            <caption>Figure 1. Steering Gear Exploded View</caption>
            <calloutgroup>
                <callout1>1. SEAL KIT - GEARS BUILT AFTER AUGUST 1, 1990 OR SERIAL NUMBER 90H1</callout1>
                <callout1>2. SEAL KIT - GEARS BUILT BEFORE AUGUST 1, 1990 OR SERIAL NUMBER 90H1</callout1>
                <callout1>3. PLUG, SLAVE PORT</callout1>
                <callout1>4. PLUG, METRIC SLAVE PORT</callout1>
                <callout1>5. BOLT AND LOCKWASHER</callout1>
                <callout1>6. PLUNGER CYLINDER HEAD (AVAILABLE AS A KIT)</callout1>
                <callout1>7. O-RING</callout1>
                <callout1>8. BLEEDER PLUG (AVAILABLE AS A KIT)</callout1>
                <callout1>9. O-RING, BLEEDER PLUG</callout1>
                <callout1>10. BOLT AND LOCKWASHER</callout1>
                <callout1>11. BOLT AND LOCKWASHER</callout1>
                <callout1>12. BRACKET</callout1>
                <callout1>13. DIRT SHIELD, OUTPUT SHAFT</callout1>
                <callout1>14. SEAL, SQUARE RING</callout1>
                <callout1>15. SEAL, SQUARE RING</callout1>
                <callout1>16. SECTOR SHAFT (AVAILABLE AS A KIT)</callout1>
                <callout1>17. LIP SEAL</callout1>
                <callout1>18. RUBBER BOOT, OUTPUT SHAFT</callout1>
                <callout1>19. BOLT</callout1>
                <callout1>20. O-RING, HOUSING COVER</callout1>
                <callout1>21. ROLLER BEARING (AVAILABLE AS A KIT)</callout1>
                <callout1>22. PITMAN ARM RETAINER (AVAILABLE AS A KIT)</callout1>
                <callout1>23. PITMAN ARM</callout1>
                <callout1>24. RING, PISTON SEAL</callout1>
                <callout1>25. O-RING, ENERGIZING</callout1>
                <callout1>26. MOLDED O-RING</callout1>
                <callout1>27. O-RING</callout1>
                <callout1>28. PISTON ASSEMBLY (AVAILABLE AS A KIT)</callout1>
                <callout1>29. CAPSCREW</callout1>
                <callout1>30. BEARING CAP (AVAILABLE AS A KIT)</callout1>
                <callout1>31. LOCKWASHER</callout1>
                <callout1>32. PRESSURE RELIEF CARTRIDGE (AVAILABLE AS A KIT)</callout1>
                <callout1>33. O-RING</callout1>
                <callout1>34. O-RING</callout1>
                <callout1>35. STRAINER</callout1>
                <callout1>36. BOLT AND LOCKWASHER</callout1>
                <callout1>37. PLUNGER, BEARING CAP (AVAILABLE AS A KIT)</callout1>
                <callout1>38. RELIEF VALVE ASSEMBLY (AVAILABLE AS A KIT)</callout1>
                <callout1>39. BOLT</callout1>
                <callout1>40. O-RING</callout1>
                <callout1>41. SEAL, SHAFT</callout1>
                <callout1>42. WASHER, BACK-UP</callout1>
                <callout1>43. SEAL, SALT</callout1>
