<isis lang="en">
    <stitle>ALLISON WT TRANSMISSION ELECTRONIC CONTROLS - COMPONENT LOCATIONS - 3600 AND 3800 MODELS</stitle>
    <svcsection/>
    <svcproc svcproctype="IDcomponent" graphiccount="6">
        <title>3600 AND 3800 MODELS</title>
        <para>Refer to
            <hotlnk xrefid="f0800363">Figure 63</hotlnk>,
            <hotlnk xrefid="f0800364">Figure 64</hotlnk>,
            <hotlnk xrefid="f0800365">Figure 65</hotlnk>,
            <hotlnk xrefid="f0800366">Figure 66</hotlnk>,
            <hotlnk xrefid="f0800367">Figure 67</hotlnk>, and
            <hotlnk xrefid="f0800368">Figure 68</hotlnk>.
        </para>
        <figure id="f0800363" figsize="pg-wide">
            <graphic filename="../figg08/g0800363.webp" type="webp" scalefit="0"/>
            <caption>Figure 63 - Transmission Wiring With 3600</caption>
            <calloutgroup>
                <callout1>1. ALLISON OVERLAY HARNESS SEE FIGURE 65</callout1>
                <callout1>2. CONNECTOR 419 DATA LINK CONNECTOR</callout1>
                <callout1>3. CONNECTOR 377 8 WAY CONNECTOR</callout1>
                <callout1>4. MAIN CAB HARNESS - REF</callout1>
                <callout1>5. CONNECTOR 420 (SHIFT SELECTOR)</callout1>
            </calloutgroup>
        </figure>
        <figure id="f0800364" figsize="pg-wide">
            <graphic filename="../figg08/g0800364.webp" type="webp" scalefit="0"/>
            <caption>Figure 64 - Transmission Wiring With 3800</caption>
            <calloutgroup>
                <callout1>1. SEE FIGURE 66</callout1>
                <callout1>2. TRANSMISSION HARNESS</callout1>
                <callout1>3. C417 AND C418</callout1>
                <callout1>4. TRANSMISSION OVERLAY HARNESS</callout1>
                <callout1>5. C420 (SHIFT SELECTOR CONNECTOR)</callout1>
                <callout1>6. C377</callout1>
                <callout1>7. C419 (DATA LINK CONNECTOR)</callout1>
            </calloutgroup>
        </figure>
        <figure id="f0800365" figsize="pg-wide">
            <graphic filename="../figg08/g0800365.webp" type="webp" scalefit="0"/>
            <caption>Figure 65 - Transmission Wiring With 3600</caption>
            <calloutgroup>
                <callout1>1. C417 &#38; 418 6 WAY CONNECTORS</callout1>
                <callout1>2. CAB OVERLAY HARNESS</callout1>
                <callout1>3. SEE FIGURE 63</callout1>
                <callout1>4. TRANSMISSION HARNESS</callout1>
                <callout1>5. SEE FIGURE 66 AND FIGURE 67</callout1>
            </calloutgroup>
        </figure>
        <figure id="f0800366" figsize="pg-wide">
            <graphic filename="../figg08/g0800366.webp" type="webp" scalefit="0"/>
            <caption>Figure 66 - Transmission Wiring With 3600 &#38; 3800</caption>
            <calloutgroup>
                <callout1>1. GROUND CIRCUIT 143 (MUST BE CONNECTED TO SAME BOLT AS FRAME GROUND STRAP)</callout1>
                <callout1>2. ECU POWER CIRCUIT 136 CONNECTS TO B+ TERMINAL</callout1>
                <callout1>3. SEE FIGURE 65</callout1>
                <callout1>4. SPARE INPUT/OUTPUT CONNECTORS C499 (GREY) &#38; A41 (BLACK) 12 WAY CONNECTORS</callout1>
                <callout1>5. ENGINE HARNESS - REF</callout1>
                <callout1>6. CIRCUIT 17 &#38; 17A NEUTRAL START</callout1>
                <callout1>7. CIRCUIT 71 &#38; 71A REVERSE WARNING</callout1>
                <callout1>8. THROTTLE POSITION SENSOR (TPS) CIRCUITS 135A, 156 &#38; 124</callout1>
                <callout1>9. TRANSMISSION TEMP SENDER CIRCUITS 31 &#38; 31 G</callout1>
                <callout1>10. SEE FIGURE 67</callout1>
                <callout1>11. TRANSMISSION HARNESS</callout1>
            </calloutgroup>
        </figure>
        <figure id="f0800367" figsize="pg-wide">
            <graphic filename="../figg08/g0800367.webp" type="webp" scalefit="0"/>
            <caption>Figure 67 - Transmission Wiring With 3600 &#38; 3800</caption>
            <calloutgroup>
                <callout1>1. CIRCUIT 144 GROUNDS ECU</callout1>
                <callout1>2. THROTTLE POSITION SENSOR (TPS)</callout1>
                <callout1>3. ENGINE HARNESS - REF</callout1>
                <callout1>4. TRANSMISSION HARNESS</callout1>
                <callout1>5. SEE FIGURE 65</callout1>
                <callout1>6. ENGINE SPEED SENSOR CIRCUITS 141 &#38; 150</callout1>
                <callout1>7. TRANSMISSION HARNESS CONNECTION</callout1>
                <callout1>8. OUTPUT SPEED SENSOR CIRCUITS 139 &#38; 148</callout1>
                <callout1>9. ELECTRONIC CONTROL UNIT (ECU) CONNECTORS "A" (BLUE) &#38; "B" (BLACK)</callout1>
                <callout1>10. SEE FIGURE 68</callout1>
            </calloutgroup>
        </figure>
        <figure id="f0800368" figsize="pg-wide">
            <graphic filename="../figg08/g0800368.webp" type="webp" scalefit="0"/>
            <caption>Figure 68 - Transmission Wiring With 3600 &#38; 3800</caption>
            <calloutgroup>
                <callout1>1. TO FIGURE 67</callout1>
                <callout1>2. TRANSMISSION HARNESS</callout1>
                <callout1>3. VEHICLE INTERFACE MODULE CONNECTORS A22 (30 WAY) &#38; A29 (18 WAY)</callout1>
            </calloutgroup>
        </figure>
    </svcproc>
