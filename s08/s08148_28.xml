    </stitle>
    <svcsection/>
    <svcproc svcproctype="IDcomponent" graphiccount="3">
        <title>VEHICLE SPEED SIGNAL</title>
        <svcprocinfo>
            <para>Refer to paragraphs
                <hotlnk document="s08148_10.xml" xrefid="x0814834">Vehicle Speed Sensor (225)</hotlnk>
