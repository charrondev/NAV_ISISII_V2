                <graphic filename="../figg08/G082901128.webp" type="webp" scalefit="1" scalefitwidth="514px"
                         scalefitheight="592px"/>
                <caption>Panel Light Outputs from EGC - Always Refer to Circuit Diagram Book for Latest Circuit
                    Information
                </caption>
                <calloutgroup>
                    <callout1>(1002) PANEL LIGHT SPLICE CONNECTOR
                        <callout2>LOCATED LEFT SIDE INSTRUMENT PANEL</callout2>
                    </callout1>
                    <callout1>(1004) GROUND SPLICE CONNECTOR
                        <callout2>LOCATED RIGHT SIDE INSTRUMENT PANEL</callout2>
                    </callout1>
