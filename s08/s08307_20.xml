<isis lang="en">
    <stitle>CF 500, CF 600 - Electrical - Warning Devices - Removal and Installation</stitle>
    <svcsection/>
    <svcproc svcproctype="remove" graphiccount="2">
        <title>Removal and Installation</title>
        <svcsubproc>
            <title>Door Ajar Switch</title>
            <step1>
                <para count="1">Open the driver or passenger door.</para>
            </step1>
            <step1>
                <para count="1">Remove the door ajar switch retaining screw.</para>
            </step1>
            <figure id="f1309005" figsize="col-wide">
                <graphic filename="../fign00/N0020218.webp" type="webp" scalefit="1" scalefitwidth="248px"
                         scalefitheight="232px"/>
            </figure>
            <step1>
                <para count="1">Disconnect the electrical connector.</para>
            </step1>
            <step1>
                <para count="1">To install, reverse the removal procedure.</para>
            </step1>
        </svcsubproc>
        <svcsubproc>
            <title>Key-In-Ignition Warning Switch</title>
            <step1>
                <para count="1">Remove the upper column shroud.</para>
            </step1>
            <step1>
                <para count="1">Press the key-in-ignition switch retaining tab.</para>
            </step1>
            <figure id="f1309008" figsize="col-wide">
                <graphic filename="../fign00/N0024980.webp" type="webp" scalefit="1" scalefitwidth="248px"
                         scalefitheight="232px"/>
            </figure>
            <step1>
                <para count="1">Remove the key-in-ignition switch.</para>
            </step1>
            <step1>
                <para count="1">Disconnect the electrical connector.</para>
            </step1>
            <step1>
                <para count="1">To install, reverse the removal procedure.</para>
            </step1>
        </svcsubproc>
    </svcproc>
