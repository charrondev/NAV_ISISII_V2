                <callout1>2. BEARING CONE</callout1>
                <callout1>3. SNAP RING</callout1>
                <callout1>4. DRIVE GEAR</callout1>
                <callout1>5. SPACER</callout1>
                <callout1>6. 4TH SPEED GEAR</callout1>
                <callout1>7. 3RD SPEED GEAR</callout1>
                <callout1>8. 2ND SPEED GEAR</callout1>
                <callout1>9. KEY</callout1>
                <callout1>10. COUNTERSHAFT</callout1>
                <callout1>11. THRUST WASHER</callout1>
                <callout1>12. BEARING</callout1>
                <callout1>13. REVERSE IDLER GEAR</callout1>
                <callout1>14. REVERSE IDLER SHAFT</callout1>
