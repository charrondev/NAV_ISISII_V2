                <truckmodel>9700</truckmodel>
                <truckmodel>9800</truckmodel>
                <ucodegroup/>

                <ucode>13GDE</ucode>
                <ucode>13GDG</ucode>
                <ucode>13GDH</ucode>
                <ucode>13GDR</ucode>
                <ucode>13GDS</ucode>
            </configure>
        </apply>
        <svcproc svcproctype="IDcomponent" graphiccount="1">
            <title>IDENTIFICATION</title>
            <para>Transmission model designation and other transmission identification information are stamped on the
                transmission tag. To identify the transmission model designation and serial number, locate the tag on
                the transmission and then locate the numbers as shown (
                <hotlnk xrefid="f1301701">Figure 1</hotlnk>).
            </para>
            <figure id="f1301701" figsize="col-wide">
                <graphic id="g1301701" filename="../figg13/G1301701.webp" type="webp"/>
                <caption>Figure 1. Transmission Identification Tag</caption>
            </figure>
            <specpara>
                <note>
                    <para>Do not remove or destroy the transmission identification tag.</para>
                </note>
            </specpara>
        </svcproc>
    </svcsection>
