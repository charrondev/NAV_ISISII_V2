<isis lang="en">

    <svcsubproc>
        <title>Transmission Filler Tube Removal</title>
        <step1>
            <para count="1">Raise and support the vehicle with the transmission in NEUTRAL. For additional information,
                refer to
                <exhotlnk document="../S10/S10019.htm" bookname="S10019">Jacking and Lifting in S10019</exhotlnk>.
            </para>
        </step1>
        <step1>
            <para count="1">Remove the fluid level indicator.</para>
        </step1>
        <figure id="f0701038" figsize="col-wide">
            <graphic filename="../fign00/N0035996.webp" type="webp" scalefit="1" scalefitwidth="248px"
                     scalefitheight="232px"/>
        </figure>
        <step1>
            <para count="1">Remove the transmission fluid fill tube nut.</para>
        </step1>
        <figure id="f0701039" figsize="col-wide">
            <graphic filename="../fign00/N0035889.webp" type="webp" scalefit="1" scalefitwidth="248px"
                     scalefitheight="232px"/>
        </figure>
        <step1>
            <para count="1">Remove the transmission fluid fill tube bracket bellhousing bolt.</para>
        </step1>
        <figure id="f0701040" figsize="col-wide">
            <graphic filename="../fign00/N0035891.webp" type="webp" scalefit="1" scalefitwidth="248px"
                     scalefitheight="232px"/>
        </figure>
        <step1>
            <para count="1">Remove the transmission filler tube from the vehicle.</para>
        </step1>
        <figure id="f0701041" figsize="col-wide">
            <graphic filename="../fign00/N0035890.webp" type="webp" scalefit="1" scalefitwidth="248px"
                     scalefitheight="232px"/>
        </figure>
        <step1>
            <para count="1">Inspect the O-ring seal for damage. Discard the O-ring if damaged.</para>
        </step1>
        <figure id="f0701042" figsize="col-wide">
            <graphic filename="../figa00/A0065044.webp" type="webp" scalefit="1" scalefitwidth="248px"
                     scalefitheight="232px"/>
        </figure>
    </svcsubproc>
