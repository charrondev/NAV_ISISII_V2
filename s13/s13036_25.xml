<isis lang="en">

    <svcsubproc>
        <title>Transmission Filler Tube Installation</title>
        <step1>
            <para count="1">Install a new O-ring if damaged.</para>
        </step1>
        <figure id="f0701043" figsize="col-wide">
            <graphic filename="../figa00/A0065044.webp" type="webp" scalefit="1" scalefitwidth="248px"
                     scalefitheight="232px"/>
        </figure>
        <specpara>
            <note>
                <para>Coat the O-ring at the bottom of the transmission fluid filler tube with transmission fluid.
                </para>
            </note>
        </specpara>
        <step1>
            <para count="1">Install the transmission fluid filler tube.</para>
        </step1>
        <figure id="f0701044" figsize="col-wide">
            <graphic filename="../fign00/N0035890.webp" type="webp" scalefit="1" scalefitwidth="248px"
                     scalefitheight="232px"/>
        </figure>
        <step1>
            <para count="1">Install the transmission fluid filler tube bellhousing bolt.</para>
            <para>&#8226; Tighten to 47 Nm (35 lb-ft).</para>
        </step1>
        <figure id="f0701045" figsize="col-wide">
            <graphic filename="../fign00/N0035889.webp" type="webp" scalefit="1" scalefitwidth="248px"
                     scalefitheight="232px"/>
        </figure>
        <step1>
            <para count="1">Install the transmission fluid filler tube bolt.</para>
            <para>&#8226; Tighten to 31 Nm (23 lb-ft).</para>
        </step1>
        <figure id="f0701046" figsize="col-wide">
            <graphic filename="../fign00/N0035891.webp" type="webp" scalefit="1" scalefitwidth="248px"
                     scalefitheight="232px"/>
        </figure>
        <step1>
            <para count="1">Install the fluid level indicator.</para>
        </step1>
        <figure id="f0701047" figsize="col-wide">
            <graphic filename="../fign00/N0035890.webp" type="webp" scalefit="1" scalefitwidth="248px"
                     scalefitheight="232px"/>
        </figure>
    </svcsubproc>
