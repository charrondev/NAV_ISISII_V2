<isis lang="en">

    <svcsubproc>
        <title>Transmission Support Crossmember Installation</title>
        <step1>
            <para count="1">Position the crossmember in place.</para>
        </step1>
        <figure id="f07010171" figsize="col-wide">
            <graphic filename="../fign00/n0020657.webp" type="webp" scalefit="1" scalefitwidth="248px"
                     scalefitheight="232px"/>
        </figure>
        <step1>
            <para count="1">Install the LH crossmember bolts.</para>
            <para>&#8226; Tighten to 81 Nm (60 lb-ft).</para>
        </step1>
        <figure id="f07010172" figsize="col-wide">
            <graphic filename="../fign00/n0020660.webp" type="webp" scalefit="1" scalefitwidth="248px"
                     scalefitheight="232px"/>
        </figure>
        <step1>
            <para count="1">Install the RH crossmember bolts and nuts.</para>
            <para>&#8226; Tighten to 81 Nm (60 lb-ft).</para>
        </step1>
        <figure id="f07010173" figsize="col-wide">
            <graphic filename="../fign00/n0020659.webp" type="webp" scalefit="1" scalefitwidth="248px"
                     scalefitheight="232px"/>
        </figure>
        <step1>
            <para count="1">Install the rear insulator nuts.</para>
            <para>&#8226; Tighten to 94 Nm (69 lb-ft).</para>
        </step1>
        <figure id="f07010174" figsize="col-wide">
            <graphic filename="../figa00/a0056172.webp" type="webp" scalefit="1" scalefitwidth="248px"
                     scalefitheight="232px"/>
        </figure>
        <step1>
            <para count="1">Install the fuel line bracket to the LH side of the crossmember and install the bolt.</para>
            <para>&#8226; Tighten to 25 Nm (18 lb-ft).</para>
        </step1>
        <figure id="f07010175" figsize="col-wide">
            <graphic filename="../fign00/n0020661.webp" type="webp" scalefit="1" scalefitwidth="248px"
                     scalefitheight="232px"/>
        </figure>
    </svcsubproc>
