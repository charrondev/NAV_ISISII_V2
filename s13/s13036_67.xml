    <svcsubproc>
        <title>Forward Hub and Ring Gear Disassembly</title>
        <step1>
            <para count="1">Remove the No. 10 forward clutch hub thrust washer.</para>
        </step1>
        <figure id="f07010333" figsize="col-wide">
            <graphic filename="../fign00/N0005657.webp" type="webp" scalefit="1" scalefitwidth="248px"
