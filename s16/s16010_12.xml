    <svcsubsec>
        <title>RUDKIN-WILEY ROOF AIR DEFLECTOR MOUNTING SYSTEM</title>
        <svcproc svcproctype="remove" graphiccount="1">
            <title>REMOVE</title>
            <step1>
                <para count="1">Disconnect channel (
                    <hotlnk xrefid="f1601024">Figure 24</hotlnk>) from front mounting support (Item 1) and rear support.
                </para>
                <figure id="f1601024" figsize="col-wide">
                    <graphic id="g1601024" filename="../figg16/G1601024.webp" type="webp" resolution="72"/>
                    <caption>Figure 24. Rudkin-Wiley Roof Air Deflector Mounting System</caption>
                    <calloutgroup>
                        <callout1>1.FRONT MOUNTING SUPPORT</callout1>
                        <callout1>2.CHANNEL</callout1>
                        <callout1>3.REAR SUPPORT</callout1>
                    </calloutgroup>
                </figure>
            </step1>
            <step1>
                <para count="1">Disconnect front and rear supports from cab roof.</para>
            </step1>
        </svcproc>
    </svcsubsec>
