                <callout1 type="arabic">ENGINE COMPARTMENT COVER</callout1>
                <callout1 type="arabic">FORWARD ENTRY ASSIST HANDRAIL</callout1>
                <callout1 type="arabic">STEP WELL LIGHT</callout1>
                <callout1 type="arabic">STEP TREADS</callout1>
