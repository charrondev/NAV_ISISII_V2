                     scalefitheight="272px"/>
            <caption>Figure 46. Step Well Main Heater Assembly</caption>
            <calloutgroup>
                <callout1 type="arabic">MAIN HEATER FACE PANEL</callout1>
                <callout1 type="arabic">ENTRY DASH PANEL TOP SURFACE PLATE</callout1>
                <callout1 type="arabic">STEP WELL GLOVE BOX</callout1>
                <callout1 type="arabic">ENTRANCE BLOWER VENTS</callout1>
                <callout1 type="arabic">STEP WELL TREAD</callout1>
            </calloutgroup>
        </figure>
