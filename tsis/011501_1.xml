<isis lang="en">
    <stitle>TSI 01-15-01 - Improved Fuel Tank Bracket Liner for Round Fuel Tanks used on 2000, 5000, 5000i, 8000, 9000
        and 9000i Models (refer to Table 1 for affected Models by Manufacturing Location, Specific Model and Build Date)
    </stitle>
    <tsi id="tsi011501" tsitype="truck">
        <tsinumber>01-15-01</tsinumber>
        <tsidate>February, 2001</tsidate>
        <subjectfile>FUEL TANKS</subjectfile>
        <subjectgroup>
            <subject>Improved Fuel Tank Bracket Liner for Round Fuel Tanks used on 2000, 5000, 5000i, 8000, 9000 and
                9000i Models (refer to Table 1 for affected Models by Manufacturing Location, Specific Model and Build
                Date)
            </subject>
        </subjectgroup>
        <apply>
            <configure>
                <truckmodelgroup/>

                <truckmodel>2000</truckmodel>
                <truckmodel>5000</truckmodel>
                <truckmodel>5000i</truckmodel>
