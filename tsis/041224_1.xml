<isis lang="en">
    <stitle>TSI 04-12-24 - New High Performance Aluminum Core Radiator Replaces Copper/Brass Radiator for HPV Models
        Built Between January 1, 2001 and November 1, 2003
    </stitle>
    <tsi id="tsi041224" tsitype="truck">
        <tsinumber>04-12-24</tsinumber>
        <tsidate>July, 2004</tsidate>
        <subjectfile>ENGINE</subjectfile>
        <subjectgroup>
            <subject>New High Performance Aluminum Core Radiator Replaces Copper/Brass Radiator for HPV Models Built
                Between January 1, 2001 and November 1, 2003
            </subject>
        </subjectgroup>
        <distribution dealer="yes" fleet="yes" bodybuilder="no" isis_cd="yes"/>
        <apply>
            <configure>
                <truckmodelgroup/>

                <truckmodel>3200
                    <daterange>
                        <startdate>01/01/2001</startdate>
                        <enddate>11/01/2003</enddate>
                    </daterange>
                </truckmodel>
                <truckmodel>4200
                    <daterange>
                        <startdate>01/01/2001</startdate>
                        <enddate>11/01/2003</enddate>
                    </daterange>
                </truckmodel>
