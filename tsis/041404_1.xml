    <stitle>TSI 04-14-04 - ArvinMeritor Rear Axle Pinion Seals</stitle>
    <tsi id="tsi041404" tsitype="truck">
        <tsinumber>04-14-04</tsinumber>
        <tsidate>August, 2004</tsidate>
        <subjectfile>REAR AXLE</subjectfile>
        <subjectgroup>
            <subject>ArvinMeritor Rear Axle Pinion Seals</subject>
        </subjectgroup>
        <distribution dealer="yes" fleet="yes" bodybuilder="no" isis_cd="yes"/>
        <apply>
            <configure>
                <vendorgroup/>

                <vendor>ArvinMeritor</vendor>
                <vendcodegroup/>

                <vendcode>A 1205 N 2588</vendcode>
                <vendcode>A 1205 Q 2591</vendcode>
                <vendcode>A 1205 P 2590</vendcode>
                <vendcode>A 1205 R 2592</vendcode>
