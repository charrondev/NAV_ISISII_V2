    <stitle>TSI 06-03-01 - Air Suspension Adjustment Specifications for Air Spring Height Dimensions and Axle Travel
        Dimensions
    </stitle>
    <tsi id="tSI060301" tsitype="truck">
        <tsinumber>06-03-01</tsinumber>
        <replace>
            <tsinumber>04-03-01</tsinumber>
        </replace>
        <tsidate>September, 2006</tsidate>
