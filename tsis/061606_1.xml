<isis lang="en">
    <stitle>TSI 06-16-06 - New Design Hood Guide and Guide Bracket to Reduce Hood Knock, Squeak, and Wear on
        International&#174; 5000i and 9000i Models Built Between April 1, 2003 and August 6, 2006
    </stitle>
    <tsi id="tsi061606" tsitype="truck">
        <tsinumber>06-16-06</tsinumber>
        <tsidate>October, 2006</tsidate>
        <subjectfile>CAB</subjectfile>
        <subjectgroup>
            <subject>New Design Hood Guide and Guide Bracket to Reduce Hood Knock, Squeak, and Wear on International&#174;
                5000i and 9000i Models Built Between April 1, 2003 and August 6, 2006
            </subject>
        </subjectgroup>
        <distribution dealer="yes" fleet="yes" bodybuilder="no" isis_cd="yes"/>
        <apply>
            <configure>
                <truckmodelgroup/>

                <truckmodel>5500i
