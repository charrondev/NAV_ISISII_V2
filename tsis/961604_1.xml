    <stitle>TSI 96-16-04 - Air Conditioning Compressor Mounting Bracket Bolt Torque on Cummins M-11 Engines</stitle>
    <tsi id="tSI961604" tsitype="truck">
        <tsinumber>96-16-04</tsinumber>
        <tsidate>December, 1996</tsidate>
        <subjectfile>CAB</subjectfile>
        <subjectgroup>
            <subject>Air Conditioning Compressor Mounting Bracket Bolt Torque on Cummins M-11 Engines</subject>
        </subjectgroup>
